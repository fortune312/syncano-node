{
  "name": "@syncano/cli",
  "version": "0.20.0",
  "description": "CLI for Syncano",
  "author": "Maciej Kucharz",
  "license": "MIT",
  "keywords": [
    "syncano",
    "baas"
  ],
  "preferGlobal": true,
  "main": "./lib/cli.js",
  "homepage": "http://syncano.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Syncano/syncano-node-cli.git"
  },
  "bin": {
    "syncano-cli": "./lib/cli.js",
    "syncano-cli-hosting": "./lib/cli-hosting.js",
    "s": "./lib/cli.js",
    "s-hosting": "./lib/cli-hosting.js"
  },
  "bugs": {
    "url": "https://github.com/Syncano/syncano-node-cli/issues"
  },
  "dependencies": {
    "analytics-node": "2.4.0",
    "archiver": "1.3.0",
    "axios": "^0.16.2",
    "babel-cli": "6.23.0",
    "babel-core": "6.21.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-extensible-destructuring": "^4.1.0",
    "babel-plugin-istanbul": "3.0.0",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-plugin-transform-regenerator": "6.24.1",
    "babel-preset-env": "^1.6.0",
    "bluebird": "3.4.6",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chalk": "1.1.3",
    "cli-table2": "0.2.0",
    "commander": "2.9.0",
    "create-error": "0.3.1",
    "debug": "2.4.5",
    "es6-template-strings": "2.0.1",
    "find-key": "2.0.1",
    "form-data": "2.1.2",
    "fs-extra": "1.0.0",
    "hashdirectory": "^0.1.0",
    "home-dir": "1.0.0",
    "inquirer": "2.0.0",
    "js-yaml": "3.8.1",
    "json-loader": "0.5.4",
    "lodash": "4.17.2",
    "md5": "2.2.1",
    "mkdirp": "0.5.1",
    "node-dir": "0.1.16",
    "node-walkup": "1.1.1",
    "npm-run-all": "3.1.2",
    "ora": "1.2.0",
    "pretty-bytes": "4.0.2",
    "raven": "1.1.4",
    "readdirp": "2.1.0",
    "semver": "5.3.0",
    "sinon": "1.17.6",
    "source-map": "0.5.6",
    "stack-trace": "0.0.9",
    "strip-ansi": "3.0.1",
    "syncano": "2.0.4",
    "unzip2": "0.2.5",
    "validator": "6.2.0",
    "watchr": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.21.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "codecov": "1.0.1",
    "condition-circle": "1.5.0",
    "console.mute": "0.3.0",
    "dirty-chai": "^2.0.1",
    "docsify-cli": "3.2.4",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.12.1",
    "mock-fs": "^4.4.2",
    "nixt": "0.5.0",
    "nyc": "^11.2.1",
    "pre-commit": "1.2.2",
    "process": "0.11.9",
    "rimraf": "2.5.4",
    "semantic-release": "6.3.2",
    "sinon": "1.17.6",
    "standard": "^10.0.3",
    "util": "0.10.3"
  },
  "scripts": {
    "prepublish": "npm run build",
    "commit": "git cz",
    "build": "babel src -d lib/ --copy-files --ignore *.test.js,*.test-e2e.js --source-maps",
    "build:watch": "yarn run build -- -w",
    "clean": "rm -rf lib/",
    "compile": "yarn run clean && yarn run build",
    "docs:watch": "docsify serve docs",
    "lint": "standard --fix --env mocha",
    "unit": "mocha 'src/**/*.test.js' --reporter spec --require babel-register -b",
    "unit:watch": "yarn run unit -- -w",
    "e2e": "mkdir -p e2e-tests/ && mkdir -p e2e-tests/hosting/ && mocha 'src/tests/e2e/*.test-e2e.js' --reporter spec --compilers js:babel-core/register --timeout 40000 --slow 8000 && rm -rf e2e-tests/",
    "e2e:tutorial": "mocha  --reporter spec --compilers js:babel-core/register --timeout 16000 --slow 8000",
    "check-coverage": "nyc yarn run unit",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "npm-run-all --parallel test:unit test:e2e",
    "test:unit": "nyc yarn run unit",
    "test:e2e": "yarn run e2e",
    "test:ci": "nyc yarn run unit -- --reporter mocha-junit-reporter",
    "reinstall": "rimraf node_modules && yarn install",
    "validate": "npm-run-all --parallel test:unit lint lint:tests build",
    "pre-commit-run": "npm-run-all --parallel lint lint:tests unit"
  },
  "pre-commit": "pre-commit-run",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "verifyConditions": {
      "path": "./deploy/condition-circle.js"
    }
  },
  "babel": {
    "plugins": [
      "add-module-exports",
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "7"
          }
        }
      ]
    ]
  }
}
