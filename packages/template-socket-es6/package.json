{
  "name": "@syncano/template-socket-es6",
  "description": "ES6 Syncano Socket Template",
  "version": "0.4.1",
  "license": "MIT",
  "files": [
    "template"
  ],
  "publishConfig": {
    "access": "public"
  },
  "templateShortDesc": "es6",
  "templateLongDesc": "ES6 Socket",
  "scripts": {
    "e2e": "mocha 'tests/e2e/*.test-e2e.js' --reporter spec --require babel-register --timeout 40000 --slow 8000",
    "test": "npm-run-all --parallel test:unit test:e2e",
    "test:e2e": "mkdir -p e2e-tests && yarn run e2e && yarn run test:e2e:clean",
    "test:e2e:clean": "rm -rf e2e-tests || 0"
  },
  "devDependencies": {
    "@syncano/cli": "0.4.1",
    "@syncano/test-tools": "0.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "codecov": "^3.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "nixt": "0.5.0",
    "nyc": "^11.3.0",
    "sinon": "1.17.6"
  },
  "babel": {
    "plugins": [
      "add-module-exports",
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  }
}
