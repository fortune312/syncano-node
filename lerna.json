{
  "lerna": "2.4.0",
  "packages": [
    "packages/*",
    "packages/registry/syncano/organization",
    "packages/registry/syncano/registry"
  ],
  "command": {
    "init": {
      "exact": true
    },
    "publish": {
      "exact": true,
      "ignore": [
        "@syncano/registry*"
      ]
    }
  },
  "version": "0.6.0",
  "npmClient": "yarn",
  "npmClientArgs": [
    "--production",
    "--no-optional",
    "--cache-folder .yarn-cache"
  ]
}
