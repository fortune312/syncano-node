{
  "name": "@syncano/cli",
  "version": "0.4.2",
  "description": "CLI for Syncano",
  "author": "Maciej Kucharz",
  "license": "MIT",
  "keywords": [
    "syncano",
    "baas"
  ],
  "preferGlobal": true,
  "main": "./lib/cli.js",
  "homepage": "http://syncano.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Syncano/syncano-node-cli.git"
  },
  "bin": {
    "syncano-cli": "./lib/cli.js",
    "syncano-cli-hosting": "./lib/cli-hosting.js",
    "s": "./lib/cli.js",
    "s-hosting": "./lib/cli-hosting.js"
  },
  "bugs": {
    "url": "https://github.com/Syncano/syncano-node-cli/issues"
  },
  "dependencies": {
    "@syncano/core": "^0.4.2",
    "@syncano/template-project-empty": "^0.4.2",
    "@syncano/template-project-hello": "^0.4.2",
    "@syncano/template-socket-es6": "^0.4.2",
    "@syncano/template-socket-vanilla": "^0.4.2",
    "analytics-node": "^3.1.1",
    "archiver": "^2.1.0",
    "axios": "^0.17.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-extensible-destructuring": "^4.1.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "cli-table2": "0.2.0",
    "commander": "^2.12.1",
    "create-error": "0.3.1",
    "cross-env": "^5.1.1",
    "debug": "2.6.9",
    "es6-template-strings": "2.0.1",
    "find-key": "2.0.1",
    "form-data": "^2.3.1",
    "fs-extra": "1.0.0",
    "hashdirectory": "^0.1.0",
    "inquirer": "^4.0.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "md5": "2.2.1",
    "mkdirp": "0.5.1",
    "node-dir": "^0.1.17",
    "node-walkup": "1.1.1",
    "npm-run-all": "^4.1.1",
    "ora": "^1.3.0",
    "pretty-bytes": "4.0.2",
    "raven": "^2.2.1",
    "readdirp": "2.1.0",
    "replace": "^0.3.0",
    "semver": "^5.4.1",
    "sinon": "1.17.6",
    "source-map": "^0.6.1",
    "stack-trace": "0.0.10",
    "strip-ansi": "^4.0.0",
    "unzip2": "0.2.5",
    "validator": "^9.1.2",
    "watchr": "3.0.1",
    "ws": "^3.3.3"
  },
  "devDependencies": {
    "@syncano/test-tools": "^0.4.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-register": "^6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "^3.0.0",
    "dirty-chai": "^2.0.1",
    "mocha": "^4.0.1",
    "mock-fs": "^4.4.2",
    "nyc": "^11.3.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "sinon": "1.17.6",
    "standard": "^10.0.3"
  },
  "scripts": {
    "prepublish": "yarn run build",
    "build": "babel src -d lib/ --copy-files --ignore *.test.js,*.test-e2e.js --source-maps",
    "build:watch": "yarn run build -- -w",
    "clean": "rm -rf lib/",
    "unit": "mocha 'tests/unit/*.test.js' --reporter spec --require babel-register",
    "unit:watch": "yarn run unit -- -w",
    "e2e": "./scripts/e2e",
    "e2e:tutorial": "mocha  --reporter spec --require babel-register --timeout 16000 --slow 8000",
    "unit:coverage": "nyc yarn run unit",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "npm-run-all --parallel test:unit test:e2e",
    "test:unit": "nyc yarn run unit",
    "test:e2e": "yarn run e2e",
    "lint": "standard --fix --env mocha",
    "format": "prettier --write --single-quote --no-semi --no-bracket-spacing *.js {src,test}/**/*.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "plugins": [
      "add-module-exports",
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "7"
          }
        }
      ]
    ]
  },
  "nyc": {
    "check-coverage": false,
    "include": [
      "src/**/*.js"
    ],
    "cache": true
  }
}
